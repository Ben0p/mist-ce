    <!-- Register Modal -->
    <div id="modalRegister" class="modal modal-mini">
        <div class="modal-title">
            <h5 class="light">
                <span class="modal-txt wide">Welcome to mist.io!</span>
                <span class="modal-txt small">Welcome!</span>
            </h5>
            <a href="javascript:void(0)" class="modal-action modal-close"><i class="mdi-navigation-close"></i></a>

            <div class="progress">
                <div class="indeterminate"></div>
            </div>
        </div>

        <div class="modal-content">
            <div class="social-statement center">
                <p>Sign up with <a href="javascript:void(0)" class="signup-gl">Google</a> or <a
                        href="javascript:void(0)" class="signup-gt">Github</a></p>
            </div>

            <div class="social-buttons">
                <a href="javascript:void(0)" class="btn-large btn-block social-btn gl signup-gl">
                    <span class="modal-txt wide">Sign up with Google</span>
                    <span class="modal-txt small">Google sign up</span>
                </a>
                <a href="javascript:void(0)" class="btn-large btn-block social-btn gt signup-gt">
                    <span class="modal-txt wide">Sign up with Github</span>
                    <span class="modal-txt small">Github sign up</span>
                </a>
            </div>

            <p class="or center">OR</p>

            <a href="javascript:void(0)" class="btn-large btn-block email-btn">
                <span class="modal-txt wide">Sign up with your Email</span>
                <span class="modal-txt small">Use your Email</span>
            </a>

            <form id="registerForm" novalidate>
                <div class="input-field">
                    <label for="signup-name">Name</label>
                    <input type="text" id="signup-name" name="name">
                </div>
                <div class="input-field">
                    <label for="signup-email">Email</label>
                    <input type="email" id="signup-email" name="email">
                </div>
                <div class="input-field right-align">
                    <button type="submit" class="btn-large black white-text">Sign Up</button>
                </div>
            </form>

            <p class="center terms">By signing up, you agree to our <a href="#modalTermsofService" class="trigger-terms">terms of service.</a></p>
        </div>
    </div>

    <!-- Signup Success Popup -->
    <div id="modalSuccess" class="modal modal-mini">
        <div class="modal-title">
            <h5 class="light">Thanks for signing up</h5>
            <a href="javascript:void(0)" class="modal-action modal-close"><i class="mdi-navigation-close"></i></a>
        </div>
        <div class="modal-content">
            <p>Dear <span class="name">friend</span>, you will soon receive an activation email. If it doesn't appear in your Inbox within a few minutes, please check your spam folder.</p>
        </div>
    </div>


    <!-- Login Modal -->
    <div id="modalLogin" class="modal modal-mini">
        <div class="modal-title">
            <h5 class="light">Sign in to your account</h5>
            <a href="javascript:void(0)" class="modal-action modal-close"><i class="mdi-navigation-close"></i></a>

            <div class="progress">
                <div class="indeterminate"></div>
            </div>
        </div>
        <div class="modal-content">
            <div class="social-statement center">
                <p>Sign in with <a href="javascript:void(0)" class="signin-gl">Google</a> or <a
                        href="javascript:void(0)" class="signin-gt">Github</a></p>
            </div>

            <div class="social-buttons">
                <a href="javascript:void(0)" class="btn-large btn-block social-btn signin-gl gl">
                    <span class="modal-txt wide">Sign in with Google</span>
                    <span class="modal-txt small">Google sign in</span>
                </a>
                <a href="javascript:void(0)" class="btn-large btn-block social-btn signin-gt gt">
                    <span class="modal-txt wide">Sign in with Github</span>
                    <span class="modal-txt small">Github sign in</span>
                </a>
            </div>

            <p class="or center">OR</p>

            <a href="javascript:void(0)" class="btn-large btn-block email-btn">Sign in with your Email</a>

            <form id="loginForm" novalidate>
                <div class="input-field">
                    <label for="signin-email">Email</label>
                    <input type="email" id="signin-email" name="email">
                </div>
                <div class="input-field">
                    <label for="signin-password">Password</label>
                    <input type="password" id="signin-password" name="password">
                </div>
                <div class="input-field right-align">
                    <button type="submit" class="btn-large black white-text">Sign In</button>
                </div>
                <div class="input-field center terms forgot-password ">
                    <a href="#modalPasswordRequest" class="modal-action modal-close modal-trigger">Forgot Password</a>
                </div>
            </form>
        </div>
    </div>

    <!-- Password Reset Request Modal -->
    <div id="modalPasswordRequest" class="modal modal-mini">
        <div class="modal-title">
            <h5 class="light">Password Reset Request</h5>
            <a href="javascript:void(0)" class="modal-action modal-close"><i class="mdi-navigation-close"></i></a>

            <div class="progress">
                <div class="indeterminate"></div>
            </div>
        </div>
        <div class="modal-content">
            <form id="passwordRequestForm" novalidate>
                <div class="input-field">
                    <label>Email</label>
                    <input type="email" name="email">
                </div>
                <div class="input-field right-align">
                    <button type="submit" class="btn-large black white-text">Submit</button>
                </div>
            </form>
        </div>
    </div>

    <!-- Password Reset Request Success Modal -->
    <div id="modalPasswordRequestSuccess" class="modal modal-mini">
        <div class="modal-title">
            <h5 class="light">Password reset requested</h5>
            <a href="javascript:void(0)" class="modal-action modal-close"><i class="mdi-navigation-close"></i></a>
        </div>
        <div class="modal-content">
            <p>You will soon receive an email with further instructions.</p>
        </div>
    </div>

    <!-- Password Set Modal -->
    <div id="modalPasswordSet" class="modal modal-mini">
        <div class="modal-title">
            <h5 class="light">Password Set</h5>
            <a href="javascript:void(0)" class="modal-action modal-close"><i class="mdi-navigation-close"></i></a>

            <div class="progress">
                <div class="indeterminate"></div>
            </div>
        </div>
        <div class="modal-content">
            <form id="passwordSetForm" novalidate>
                <div class="input-field">
                    <label for="password">Enter Password</label>
                    <input type="password" id="password" name="password">
                </div>
                <div class="input-field">
                    <label for="confirm_password">Confirm Password</label>
                    <input type="password" id="confirm_password" name="confirm_password">
                </div>
                <div class="input-field right-align">
                    <button type="submit" class="btn-large black white-text">Submit</button>
                </div>
            </form>
        </div>
    </div>

    <!-- Password Reset Modal -->
    <div id="modalPasswordReset" class="modal modal-mini">
        <div class="modal-title">
            <h5 class="light">Password Reset</h5>
            <a href="javascript:void(0)" class="modal-action modal-close"><i class="mdi-navigation-close"></i></a>

            <div class="progress">
                <div class="indeterminate"></div>
            </div>
        </div>
        <div class="modal-content">
            <form id="passwordResetForm" novalidate>
                <div class="input-field">
                    <label for="password">Enter Password</label>
                    <input type="password" id="reset_password" name="password">
                </div>
                <div class="input-field">
                    <label for="confirm_password">Confirm Password</label>
                    <input type="password" id="reset_confirm_password" name="confirm_password">
                </div>
                <div class="input-field right-align">
                    <button type="submit" class="btn-large black white-text">Submit</button>
                </div>
            </form>
        </div>
    </div>


