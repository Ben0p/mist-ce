<ul class="actions" >
    <li id="create"><a class="enabled">Allocate</a></li>
    <li id="start"><a class="enabled">Associate</a></li>
    <li id="reboot"><a class="enabled">Disassociate</a></li>
    <li id="destroy"><a class="enabled">Release</a></li>
</ul>

<div class="networks">
     <div tal:repeat="net networks">
<!--        <div class="node" tal:attributes="id net.id">-->
        <h3 tal:content="net"></h3>
        <b> Private IPs </b>
        <input type="checkbox" tal:attributes="id string:chk-${networks[net]['private_ip']}" />
        <span tal:content="networks[net]['private_ip']">test</span> <br/>
        <b> Public IPs </b>
        <input type="checkbox" tal:attributes="id string:chk-${networks[net]['public_ip']}" />
        <span tal:content="networks[net]['public_ip']">test</span>
<!--        </div>-->
        </div>
        <span tal:content="networks">test</span>
        <hr />
</div>
