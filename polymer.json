{
  "entrypoint": "index.html",
  "shell": "src/landing-app.html",
  "fragments": [
   "src/lazy-resources.html",
   "src/landing-home.html",
   "src/landing-sign-in.html",
   "src/landing-sign-up.html",
   "src/landing-set-password.html",
   "src/landing-forgot-password.html",
   "src/landing-reset-password.html",
   "src/landing-product.html",
   "src/landing-about.html",
   "src/landing-rbac.html",
   "src/landing-orchestration.html",
   "src/landing-pricing.html"

  ],
  "sources": [
   "src/**/*",
   "data/**/*",
   "images/**/*",
   "bower.json",
   "app.yaml"
  ],
  "extraDependencies": [
    "manifest.json",
    "bower_components/webcomponentsjs/**/*",
    "bower_components/custom-elements/**/*",
    "bower_components/shadydom/**/*",
    "bower_components/shadycss/**/*"
  ],
  "builds": [{
    "name": "bundled",
    "bundle": true,
    "js": {"minify": true},
    "css": {"minify": true},
    "html": {"minify": true}
  },{
    "name": "unbundled",
    "js": {"minify": true},
    "css": {"minify": true},
    "html": {"minify": true}
  }]
}
